{
  "name": "@jubalm/profiler",
  "version": "0.0.0",
  "description": "Lightweight project profiler that generates .profile files for LLM context",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "profiler": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "profile": "tsx src/index.ts",
    "check": "npx @biomejs/biome check --write .",
    "ci": "npx @biomejs/biome ci .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "profiler",
    "project",
    "detection",
    "llm",
    "context"
  ],
  "author": "jubalm",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist"
  ]
}
